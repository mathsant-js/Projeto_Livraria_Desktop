<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="265.0" prefWidth="425.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaLoginController">
   <children>
      <Label fx:id="tituloLogin" layoutX="140.0" layoutY="14.0" text="Bem-vindo de volta!">
         <font>
            <Font name="Dubai Regular" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="campoNome" layoutX="37.0" layoutY="85.0" prefHeight="32.0" prefWidth="351.0" promptText="Nome completo..." />
      <Label fx:id="rotuloNome" layoutX="37.0" layoutY="63.0" text="Digite seu nome:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="rotuloSenha" layoutX="37.0" layoutY="134.0" text="Digite sua senha:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="campoSenha" layoutX="37.0" layoutY="156.0" prefHeight="32.0" prefWidth="351.0" promptText="Senha" />
      <Button fx:id="botaoEntrar" layoutX="176.0" layoutY="207.0" mnemonicParsing="false" onAction="#botaoEntrarAction" prefHeight="25.0" prefWidth="73.0" text="Entrar">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
